{
  "version": 2,
  "name": "accvaultng-backend",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "MONGODB_URI": "@mongodb_uri",
    "JWT_SECRET": "@jwt_secret",
    "PAYSTACK_PUBLIC_KEY": "@paystack_public_key",
    "PAYSTACK_SECRET_KEY": "@paystack_secret_key",
    "FRONTEND_URL": "@frontend_url"
  }
}